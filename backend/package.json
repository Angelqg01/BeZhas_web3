{
  "name": "bezhas-backend",
  "version": "1.0.0",
  "description": "Secure backend for BeZhas Web3 social network",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "chat": "node chat/chat-server.js",
    "chat:dev": "nodemon chat/chat-server.js",
    "test": "jest --env=node",
    "test:watch": "jest --env=node --watch",
    "test:coverage": "jest --env=node --coverage",
    "test:ci": "jest --env=node --coverage --ci --reporters=default --reporters=jest-junit",
    "test:unit": "jest --env=node --testPathPattern=tests/(?!integration)",
    "test:integration": "jest --env=node --testPathPattern=tests/integration",
    "test:db": "jest --env=node tests/database-connection.test.js",
    "test:payments": "jest --env=node tests/payment-flow-e2e.test.js",
    "test:all": "npm run test:db && npm run test:payments",
    "test:chat": "node chat/test-chat-integration.js",
    "lint": "eslint . --ext .js",
    "check-updates": "npm outdated",
    "update-safe": "npm update",
    "clean": "rm -rf node_modules package-lock.json && npm install",
    "seed:dao": "node seeds/daoSeed.js",
    "health": "curl -s http://localhost:3000/api/health | node -e \"console.log(JSON.parse(require('fs').readFileSync('/dev/stdin').toString()))\""
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.67.0",
    "@google/generative-ai": "^0.24.1",
    "@octokit/rest": "^22.0.1",
    "@otplib/preset-default": "^12.0.1",
    "@react-oauth/google": "^0.13.4",
    "@simplewebauthn/server": "^13.2.2",
    "arweave": "^1.15.1",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "bullmq": "^5.66.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "ethers": "^6.16.0",
    "express": "^4.22.1",
    "express-pino-logger": "^7.0.0",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.18.2",
    "express-validator": "^7.2.1",
    "form-data": "^4.0.5",
    "gcp-metadata": "^5.3.0",
    "glob": "^13.0.0",
    "google-auth-library": "^10.5.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.3",
    "mongoose": "^8.20.3",
    "multer": "^2.0.2",
    "nanoid": "^5.1.6",
    "natural": "^8.1.0",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.3",
    "openai": "^6.15.0",
    "otplib": "^13.2.1",
    "pino": "^10.3.1",
    "pino-pretty": "^11.3.0",
    "path-to-regexp": "0.1.7",
    "prom-client": "^15.1.3",
    "qrcode": "^1.5.4",
    "rss-parser": "^3.13.0",
    "speakeasy": "^2.0.0",
    "stripe": "^14.25.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tslib": "^2.8.1",
    "uuid": "^9.0.1",
    "validator": "^13.11.0",
    "winston": "^3.19.0",
    "ws": "^8.14.2",
    "zod": "^3.25.76",
    "@bezhas/sdk": "file:./bezhas-sdk-2.0.0.tgz",
    "@bezhas/mcp-server": "file:./bezhas-mcp-server-1.0.0.tgz"
  },
  "devDependencies": {
    "@babel/traverse": "^7.29.0",
    "@prisma/client": "5.10.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.11",
    "prisma": "5.10.0",
    "supertest": "^7.0.0"
  },
  "keywords": [
    "blockchain",
    "web3",
    "social-network",
    "security"
  ],
  "author": "BeZhas Team",
  "license": "MIT"
}