{
  "name": "bezhas-web3",
  "version": "1.0.0",
  "description": "Web3 Application for GCP deployment",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "echo \"No tests specified yet\" && exit 0",
    "docker:build": "docker build -t bezhas-web3 .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "gcp:setup": "bash scripts/setup-gcp.sh",
    "gcp:deploy": "gcloud run deploy bezhas-web3 --source .",
    "local:dev": "bash scripts/local-dev.sh"
  },
  "keywords": [
    "web3",
    "gcp",
    "cloud-run",
    "docker",
    "mongodb"
  ],
  "author": "BeZhas Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "dotenv": "^16.3.1",
    "mongodb": "^6.3.0",
    "web3": "^4.3.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
