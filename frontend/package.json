  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "serve -s dist -l 8080",
    "build": "node --max-old-space-size=4096 ./node_modules/vite/bin/vite.js build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
